<%= form_with model: @user_song, method: :patch, url: rate_song_path, html: do |f| %>
    <%= f.hidden_field :user_id, :value => current_user.id %>
    <%= f.hidden_field :song_id, :value => @song.id  %>
    <%= f.label :rating, "Your Rating" %>
    <%= f.number_field :rating, in: 1..5 %>
    <%= f.submit "Update Rating" %>
<% end %> 