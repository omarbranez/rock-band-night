<h1> Sign up for a new account</h1>
<%= form_with model: @user do |f| %>
    <%= f.label :username %> 
    <%= f.text_field :username %> <br>
    <%= f.label :password %>
    <%= f.password_field :password %> <br>
    <%= f.label :password_confirmation %> 
    <%= f.password_field :password_confirmation %> <br>
<%= f.submit %>
<% end %>

